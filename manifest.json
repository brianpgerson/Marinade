{
  "manifest_version": 2,
  "name": "Marinade",
  "description": "This extension makes your bookmarks better.",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "https://ajax.googleapis.com/"
  ]
}